query => {\n      if (query.matches) {\n        console.log(qId)\n        Noodl.Variables['Active Media Queries'].push(qId)\n      } else {\n        Noodl.Variables['Active Media Queries'] = Noodl.Variables['Active Media Queries'].filter(q => q !== qId)\n      }\n    }\n\n    this.createdQueries.push(qId)\n    this.queries[qId] = window.matchMedia(queryObject.query)\n    this.listeners[qId](this.queries[qId])\n    this.queries[qId].addEventListener('change', this.listeners[qId])\n  }\n\n  queryObjects.forEach((queryObject) => {\n    this.createListener(queryObject)\n  })    \n}\n\nNode.OnDestroy = function() {\n    if(this.createdQueries.length) {\n      this.createdQueries.forEach(qId => {\n        this.queries[qId].removeEventListener('change', this.listeners[qId])\n        this.queries[qId] = undefined\n        this.listeners[qId] = undefined\n        this.createdQueries = this.createdQueries.filter(query => query !== qId)\n      })\n    }\n}","scriptInputs":[],"scriptOutputs":[],"outtype-match":"boolean","outtype-noMatch":"boolean","intype-mediaQuery":"object","intype-mediaQueries":"array"},"ports":[],"dynamicports":[],"children":[],"metadata":{"merge":{"soureCodePorts":["code"]}}},{"id":"aeb9d188-c13f-0a8c-2d8e-64a8f940873e","type":"Variable2","x":295,"y":-17,"parameters":{"name":"Active Media Queries"},"ports":[],"dynamicports":[],"children":[]}],"visualRoots":[],"comments":[{"text":"Media queries are created and cleaned up here. \n\nIf you add queries, make sure to include them in the \"Match Media Query\" component as well.","width":243,"height":220,"fill":true,"x":23,"y":-156,"id":"767b3626-196f-671a-70d0-87a7ecc9b8de"},{"text":"Used to store an array of the identifiers for the media queries that are currently active","width":243,"height":216,"fill":true,"x":284,"y":-156,"id":"6d6a9134-d868-4eac-fe20-3531909184c2"},{"text":"IMPORTANT:\n\nThis component should be placed in your home component. \n\nIt should not be used more than once.","width":279,"height":2